/ {
    gpiocustom {
        compatible = "gpio-keys";
        gpiocus0: gpiocus_0 {
            gpios = <&gpio1 5 GPIO_ACTIVE_LOW>;
            label = "Custom gpio 28";
        };
        gpiocus1: gpiocus_1 {
            gpios = <&gpio1 3 GPIO_ACTIVE_LOW>;
            label = "Custom gpio 29";
        };
    };
    aliases {
        gpiocus0 = &gpiocus0;
        gpiocus1 = &gpiocus1;
    };
};